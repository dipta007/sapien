{
  "name": "node.js-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha src/**/tests/*.js --timeout 30000",
    "install:prod": "NODE_ENV=production npm install",
    "start": "node src/index.js",
    "lint": "eslint ./src/ --cache",
    "pretty": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "precommit": "npm run lint && npm test",
    "start:prod": "NODE_ENV=production pm2 start app.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipta007/node.js-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dipta007/node.js-boilerplate/issues"
  },
  "homepage": "https://github.com/dipta007/node.js-boilerplate#readme",
  "dependencies": {
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "aws-sdk": "^2.415.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.3",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "logdna": "^2.1.1",
    "multer": "^1.4.1",
    "pg": "^7.8.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
